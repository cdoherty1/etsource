- use = energetic
- energy_balance_group = final consumption
- groups = [co2_emissions_primary]
- graph_methods = [demand]
- free_co2_factor = 0.0

~ demand =
    EB("residential", biogasoline) +
    EB("residential", other_liquid_biofuels) +
    EB("residential", "non-specified_primary_biofuels_and_waste")
